{
  "name": "chatgpt-app-server",
  "version": "1.0.0",
  "description": "MCP server for ChatGPT app template",
  "type": "module",
  "main": "dist/server.js",
  "exports": {
    ".": "./dist/server.js",
    "./types": "./src/types.ts"
  },
  "scripts": {
    "dev": "tsx watch src/server.ts",
    "build": "tsc",
    "start": "node dist/server.js",
    "test": "vitest run",
    "test:watch": "vitest",
    "test:coverage": "vitest run --coverage"
  },
  "dependencies": {
    "@modelcontextprotocol/sdk": "^1.25.1",
    "dotenv": "^17.2.3",
    "express": "^5.2.1",
    "pino": "^10.1.0",
    "pino-http": "^11.0.0",
    "uuid": "^13.0.0",
    "zod": "^4.2.1"
  },
  "devDependencies": {
    "@types/express": "^5.0.6",
    "@types/node": "^25.0.3",
    "pino-pretty": "^13.1.3",
    "vitest": "^4.0.16"
  }
}
